{
  "name": "prepdock",
  "version": "1.0.0",
  "main": "electron/main.js",
  "type": "module",
  "productName": "PrepDock",
  "author": "PrepDock Inc.",
  "build": {
    "appId": "com.prepdock.app",
    "asar": true,
    "files": [
      "dist/**",
      "dist-electron/**",
      "electron/**",
      "resources/**",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "resources/",
        "to": "resources",
        "filter": ["**/*"]
      }
    ]
  },
  "scripts": {
    "dev": "vite",
    "dev:electron": "electron electron/main.js",
    "dev:all": "concurrently --raw -k \"npm:dev\" \"npm:dev:electron\"",
    "build:renderer": "vite build",
    "lint": "eslint --ext .ts,.js .",
    "type-check": "tsc --noEmit",
    "build": "npm run build:renderer && electron-builder --dir",
    "start": "npm run build:renderer && electron electron/main.js"
  },
  "dependencies": {
    "assemblyai": "^4.9.0",
    "child_process": "^1.0.2",
    "dotenv": "^16.5.0",
    "electron": "^29.1.4",
    "form-data": "^4.0.2",
    "formdata-node": "^6.0.3",
    "marked": "^15.0.8",
    "node-fetch": "^3.3.2",
    "ocr-space-api-wrapper": "^2.3.2",
    "open": "^10.1.2",
    "openai": "^4.95.1",
    "screenshot-desktop": "^1.15.1",
    "sharp": "^0.34.1",
    "vite": "^6.2.5",
    "vite-plugin-electron": "^0.28.4",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@types/node": "^22.14.1",
    "@types/screenshot-desktop": "^1.12.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "eslint": "^8.56.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-config-prettier": "^9.1.0",
    "@typescript-eslint/parser": "^7.10.0",
    "@typescript-eslint/eslint-plugin": "^7.10.0",
    "prettier": "^3.2.5",
    "concurrently": "^8.2.2",
    "husky": "^9.0.0",
    "lint-staged": "^15.2.0"
  },
  "prepare": "husky install",
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": ["eslint --fix", "prettier --write"]
  }
}
